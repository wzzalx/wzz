  if(CMAKE_ERROR_ON_ABSOLUTE_INSTALL_DESTINATION)
    message(FATAL_ERROR "ABSOLUTE path INSTALL DESTINATION forbidden (by caller): ${CMAKE_ABSOLUTE_DESTINATION_FILES}")
  endif()
file(INSTALL DESTINATION "/usr/local/eosio/include/eosio/testing" TYPE FILE FILES
    "/opt/eos2/eos/libraries/testing/include/eosio/testing/chainbase_fixture.hpp"
    "/opt/eos2/eos/libraries/testing/include/eosio/testing/tester.hpp"
    "/opt/eos2/eos/libraries/testing/include/eosio/testing/tester_network.hpp"
    )
endif()

if("x${CMAKE_INSTALL_COMPONENT}x" STREQUAL "xUnspecifiedx" OR NOT CMAKE_INSTALL_COMPONENT)
  list(APPEND CMAKE_ABSOLUTE_DESTINATION_FILES
   "/usr/local/eosio/include/eosio/testing/")
  if(CMAKE_WARN_ON_ABSOLUTE_INSTALL_DESTINATION)
